CREATE TABLE `rankingengine_wordpress`.`re_user_progress` ( `progress_id` INT(11) NOT NULL AUTO_INCREMENT , `wpuid` INT(11) NOT NULL , `progress_desc` TEXT CHARACTER SET utf8mb4 COLLATE utf8mb4_unicode_ci NOT NULL , `created_date` DATE NOT NULL , `save_date` DATE NOT NULL , `percent_complete` INT(11) NOT NULL , `item_count` INT(11) NOT NULL , `progress_data` MEDIUMTEXT CHARACTER SET utf8mb4 COLLATE utf8mb4_unicode_ci NOT NULL , `re_version` VARCHAR(11) NOT NULL , PRIMARY KEY (`progress_id`)) ENGINE = MyISAM CHARSET=utf8mb4 COLLATE utf8mb4_unicode_ci;