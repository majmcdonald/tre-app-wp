CREATE TABLE `rankingengine_wordpress`. ( `template_id` INT(11) NOT NULL AUTO_INCREMENT , `wpuid` INT(11) NOT NULL , `template_desc` TEXT CHARACTER SET utf8mb4 COLLATE utf8mb4_unicode_ci NOT NULL , `created_date` DATE NOT NULL , `updated_date` DATE NOT NULL , `item_count` INT(11) NOT NULL , `rank_count` INT(11) NOT NULL , `template_data` MEDIUMTEXT CHARACTER SET utf8mb4 COLLATE utf8mb4_unicode_ci NOT NULL , `re_version` VARCHAR(11) NOT NULL , PRIMARY KEY (`template_id`)) ENGINE = MyISAM CHARSET=utf8mb4 COLLATE utf8mb4_unicode_ci;