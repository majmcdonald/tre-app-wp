CREATE TABLE `rankingengine_wordpress`.`re_user_results1` ( `result_id` INT(11) NOT NULL AUTO_INCREMENT , `wpuid` INT(11) NOT NULL , `result_desc` TEXT CHARACTER SET utf8mb4 COLLATE utf8mb4_unicode_ci NOT NULL , `finish_date` DATE NOT NULL , `item_count` INT(11) NOT NULL , `list_category` INT(11) NOT NULL , `result_data` MEDIUMTEXT CHARACTER SET utf8mb4 COLLATE utf8mb4_unicode_ci NOT NULL , `re_version` VARCHAR(11) CHARACTER SET utf8mb4 COLLATE utf8mb4_unicode_ci NOT NULL , PRIMARY KEY (`result_id`)) ENGINE = MyISAM CHARSET=utf8mb4 COLLATE utf8mb4_unicode_ci