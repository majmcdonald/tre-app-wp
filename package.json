{
  "name": "the-ranking-engine",
  "version": "2.0.0",
  "description": "",
  "main": "input.js",
  "scripts": {
    "dev-server": "webpack-dev-server --mode development --browser=firefox",
    "build": "webpack --mode production",
    "watch:sass": "node-sass src/styles/main.scss public/styles/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel dev-server watch:sass",
    "compile:sass": "node-sass src/styles/main.scss public/styles/style.compile.css -w",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" public/styles/style.compile.css -o public/styles/style.prefix.css",
    "compress:css": "node-sass public/styles/style.prefix.css public/styles/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "live-server": "^1.2.0",
    "uuidv4": "^2.0.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.3"
  },
  "devDependencies": {
    "eslint": "^5.13.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "node-sass": "^4.11.0",
    "autoprefixer": "^7.1.4",
    "concat": "^1.0.3",
    "live-server": "^1.2.1",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^4.1.1"
  }
}
